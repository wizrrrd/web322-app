<div class="row">
    <div class="col-md-12">
        <h2>
            <%= session.user.userName %> ( <%= session.user.email %> ) History
        </h2>
        <hr />

        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Login Date/Time</th>
                        <th>Client Information</th>
                    </tr>
                </thead>
                <tbody>
                    <% session.user.loginHistory.forEach(function(historyItem) { %>
                        <tr>
                            <td>
                                <%= historyItem.dateTime %>
                            </td>
                            <td>
                                <%= historyItem.userAgent %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>